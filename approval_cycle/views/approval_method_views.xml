<odoo>
    <record id="view_studio_approval_method_form" model="ir.ui.view">
        <field name="name">studio.approval.method.form</field>
        <field name="model">studio.approval.method</field>
        <field name="arch" type="xml">
            <form string="Approval Method">
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="label"/>
                        <field name="model_id"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="view_studio_approval_method_tree" model="ir.ui.view">
        <field name="name">studio.approval.method.tree</field>
        <field name="model">studio.approval.method</field>
        <field name="arch" type="xml">
            <list>
                <field name="label"/>
                <field name="name"/>
                <field name="model_id"/>
            </list>
        </field>
    </record>
</odoo>